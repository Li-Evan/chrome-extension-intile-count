{
  "manifest_version": 3,
  "name": "Intitle结果数量查询",
  "version": "1.0.0",
  "description": "查询Google搜索中特定词的intitle结果数量",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "sidePanel"
  ],
  "host_permissions": [
    "https://www.google.com/*",
    "https://www.google.com.hk/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}

